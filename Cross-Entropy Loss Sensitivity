{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[],"machine_shape":"hm","gpuType":"A100","authorship_tag":"ABX9TyNpAopn8VnEGs4KtQsbii+Z"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"},"accelerator":"GPU"},"cells":[{"cell_type":"code","source":["# ============================================================\n","# 0.  One‑off upgrade so WikiText‑103 loads LOCALLY\n","# ============================================================\n","import subprocess, sys, math, random, torch, torch.nn.functional as F\n","subprocess.run(\n","    [sys.executable, \"-m\", \"pip\", \"install\", \"-q\",\n","     \"--upgrade\", \"datasets>=2.18.0\", \"fsspec>=2023.6.0\"],\n","    check=True\n",")\n","\n","# ============================================================\n","# 1.  Imports & config\n","# ============================================================\n","from datasets     import load_dataset\n","from transformers import AutoTokenizer, GPT2LMHeadModel\n","\n","DEVICE      = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","MODEL_ID    = \"gpt2\"\n","SEQ_LEN     = 1_024\n","BATCH_SIZE  = 16\n","MAX_STEPS   = 5          # mini‑batches for gradient + PPL\n","TOP_K       = 3           # keep K most‑sensitive scalars\n","V_SELECT    = \"all\"       # \"all\" | int (1‑based) | list of ints\n","NUM_TRIALS  = 5           # flips per bit‑class, per selected scalar\n","\n","TARGET      = \"transformer.h.0.mlp.c_fc.weight\"   # weight tensor to probe\n","\n","# ============================================================\n","# 2.  Model & tokenizer\n","# ============================================================\n","tok = AutoTokenizer.from_pretrained(MODEL_ID)\n","tok.pad_token = tok.eos_token\n","model = GPT2LMHeadModel.from_pretrained(MODEL_ID).to(DEVICE)\n","model.eval()\n","\n","# ============================================================\n","# 3.  Load WikiText‑103 locally\n","# ============================================================\n","wiki = load_dataset(\"wikitext\", \"wikitext-103-raw-v1\", split=\"train\")\n","\n","def chunk_generator():\n","    cache = []\n","    for doc in wiki:\n","        cache.extend(tok(doc[\"text\"]).input_ids)\n","        while len(cache) >= SEQ_LEN + 1:\n","            win, cache = cache[:SEQ_LEN + 1], cache[SEQ_LEN + 1:]\n","            yield win[:-1], win[1:]\n","\n","def get_batch(gen, bs=BATCH_SIZE):\n","    buf_x, buf_y = [], []\n","    for x, y in gen:\n","        buf_x.append(x); buf_y.append(y)\n","        if len(buf_x) == bs:\n","            yield (torch.tensor(buf_x, device=DEVICE),\n","                   torch.tensor(buf_y, device=DEVICE))\n","            buf_x, buf_y = [], []\n","\n","# ============================================================\n","# 4.  Scan MAX_STEPS batches → find top‑K |∂L/∂θ|\n","# ============================================================\n","W = dict(model.named_parameters())[TARGET]\n","running_max = torch.zeros_like(W)\n","eval_inputs = []                               # keep windows for PPL\n","\n","for step, (inp, _) in enumerate(get_batch(chunk_generator()), 1):\n","    eval_inputs.append(inp)                    # save for evaluation\n","    model.zero_grad(set_to_none=True)\n","    loss = model(inp, labels=inp).loss         # HF shifts internally for input; compares with label to find loss\n","    loss.backward()\n","    running_max = torch.maximum(running_max, W.grad.abs())  # finds the maximum gradient every element has reached\n","    if step >= MAX_STEPS:\n","        break\n","\n","flat_vals, flat_idx = torch.topk(running_max.view(-1), k=TOP_K)\n","coords_list = [torch.unravel_index(i, W.shape) for i in flat_idx]\n","\n","print(f\"\\nTop‑{TOP_K} |∂L/∂θ| entries in {TARGET}:\")\n","for rank, (val, c) in enumerate(zip(flat_vals.tolist(), coords_list), 1):\n","    print(f\"  #{rank:2d}: {TARGET}{tuple(map(int,c))}  |grad|={val:.3e}\")\n","\n","def normalize_v_select(v_sel, k):\n","    if v_sel == \"all\":          return list(range(1, k+1))\n","    if isinstance(v_sel, int):  return [v_sel]\n","    if isinstance(v_sel, (list, tuple)): return list(v_sel)\n","    raise ValueError(\"V_SELECT must be 'all', int, or list[int]\")\n","\n","ranks_to_test = normalize_v_select(V_SELECT, TOP_K)\n","print(f\"\\nTesting ranks: {ranks_to_test}\")\n","\n","# ============================================================\n","# 5.  Perplexity helper on saved windows\n","# ============================================================\n","@torch.no_grad()\n","def ppl_over_inputs(batches):\n","    total_nll, total_tok = 0.0, 0\n","    for x in batches:\n","        logits = model(x[:, :-1]).logits\n","        tgt    = x[:, 1:]                      # next‑token labels\n","        nll = F.cross_entropy(\n","                logits.reshape(-1, logits.size(-1)),\n","                tgt.reshape(-1),\n","                reduction=\"sum\"\n","              )\n","        total_nll += nll.item()\n","        total_tok += tgt.numel()\n","    return math.exp(total_nll / total_tok)\n","\n","baseline_ppl = ppl_over_inputs(eval_inputs)\n","print(f\"\\nBaseline PPL over {len(eval_inputs)*BATCH_SIZE} windows: \"\n","      f\"{baseline_ppl:.4f}\")\n","\n","# ============================================================\n","# 6.  Bit‑flip helpers\n","# ============================================================\n","def flip_bit(val_tensor: torch.Tensor, bit: int):\n","    int_view = val_tensor.view(torch.int32)\n","    int_view ^= (1 << bit)\n","    return int_view.view(torch.float32)\n","\n","BIT_CLASSES = {\n","    \"sign\"     : [31],\n","    \"exponent\" : list(range(23, 31)),\n","    \"mantissa\" : list(range(0, 23)),\n","}\n","\n","# ============================================================\n","# 7.  Flip bits – skip NaNs but record failures\n","# ============================================================\n","overall = {}   # {rank: {cls: {\"mean\":..., \"fails\":...}}}\n","\n","for rank in ranks_to_test:\n","    coord     = coords_list[rank-1]\n","    orig_val  = W.data[coord].clone()\n","    stats_cls = {}\n","\n","    for cls, pool in BIT_CLASSES.items():\n","        deltas, fails = [], 0\n","        for _ in range(NUM_TRIALS):\n","            bit = random.choice(pool)\n","            W.data[coord] = flip_bit(orig_val, bit)\n","            ppl_corrupt   = ppl_over_inputs(eval_inputs)\n","            if math.isfinite(ppl_corrupt):\n","                deltas.append(ppl_corrupt - baseline_ppl)\n","            else:\n","                fails += 1\n","            W.data[coord] = orig_val\n","\n","        mean_delta = (sum(deltas) / len(deltas)) if deltas else float(\"nan\")\n","        stats_cls[cls] = {\"mean\": mean_delta, \"fails\": fails}\n","\n","    overall[rank] = stats_cls\n","\n","# ============================================================\n","# 8.  Report\n","# ============================================================\n","print(\"\\n=== Mean Δ‑PPL (skipping NaNs) ===\")\n","for rank in sorted(overall):\n","    r, c = map(int, coords_list[rank-1])\n","    print(f\"\\nRank #{rank}  ({TARGET}({r},{c}))\")\n","    for cls, st in overall[rank].items():\n","        mean_str = f\"{st['mean']:+.4f}\" if math.isfinite(st['mean']) else \"NaN\"\n","        print(f\"  {cls:9s}: {mean_str:>8}   \"\n","              f\"(skipped {st['fails']}/{NUM_TRIALS})\")\n","\n","# === Optional aggregate view ===\n","if len(ranks_to_test) > 1:\n","    agg = {cls: [] for cls in BIT_CLASSES}\n","    for fields in overall.values():\n","        for cls, st in fields.items():\n","            if math.isfinite(st[\"mean\"]):\n","                agg[cls].append(st[\"mean\"])\n","    agg_mean = {cls: (sum(v)/len(v) if v else float(\"nan\"))\n","                for cls, v in agg.items()}\n","    worst = max(agg_mean, key=lambda k: (agg_mean[k]\n","                                         if math.isfinite(agg_mean[k])\n","                                         else -float(\"inf\")))\n","    if math.isfinite(agg_mean[worst]):\n","        print(f\"\\nOverall most sensitive (finite) field: **{worst.upper()}**\")\n","    else:\n","        print(\"\\nOverall most sensitive field: **UNDEFINED (all NaN)**\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Fug9BJUHS9OR","outputId":"a9c40f56-4dc6-4f89-fa69-b326be05322b","executionInfo":{"status":"ok","timestamp":1753465043404,"user_tz":-330,"elapsed":75564,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}}},"execution_count":3,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Top‑3 |∂L/∂θ| entries in transformer.h.0.mlp.c_fc.weight:\n","  # 1: transformer.h.0.mlp.c_fc.weight(138, 2454)  |grad|=1.217e-02\n","  # 2: transformer.h.0.mlp.c_fc.weight(393, 2454)  |grad|=1.214e-02\n","  # 3: transformer.h.0.mlp.c_fc.weight(266, 2454)  |grad|=1.181e-02\n","\n","Testing ranks: [1, 2, 3]\n","\n","Baseline PPL over 80 windows: 32.1660\n","\n","=== Mean Δ‑PPL (skipping NaNs) ===\n","\n","Rank #1  (transformer.h.0.mlp.c_fc.weight(138,2454))\n","  sign     :  -0.0238   (skipped 0/5)\n","  exponent :  -0.0317   (skipped 0/5)\n","  mantissa :  -0.0199   (skipped 0/5)\n","\n","Rank #2  (transformer.h.0.mlp.c_fc.weight(393,2454))\n","  sign     :  +0.0156   (skipped 0/5)\n","  exponent : +108.4633   (skipped 0/5)\n","  mantissa : +180.7557   (skipped 0/5)\n","\n","Rank #3  (transformer.h.0.mlp.c_fc.weight(266,2454))\n","  sign     : +180.7557   (skipped 0/5)\n","  exponent : +180.7557   (skipped 0/5)\n","  mantissa : +180.7557   (skipped 0/5)\n","\n","Overall most sensitive (finite) field: **MANTISSA**\n"]}]},{"cell_type":"code","source":["# ============================================================\n","# 0. One‑off upgrade so WikiText‑103 loads LOCALLY\n","# ============================================================\n","import subprocess, sys, math, random, torch, torch.nn.functional as F\n","subprocess.run(\n","    [sys.executable, \"-m\", \"pip\", \"install\", \"-q\",\n","     \"--upgrade\", \"datasets>=2.18.0\", \"fsspec>=2023.6.0\"],\n","    check=True\n",")\n","\n","# ============================================================\n","# 1. Imports & config\n","# ============================================================\n","from datasets     import load_dataset\n","from transformers import AutoTokenizer, GPT2LMHeadModel\n","\n","DEVICE      = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","MODEL_ID    = \"gpt2\"\n","SPLIT       = \"validation\"   # use \"train\" if you want lower baseline PPL (~30)\n","SEQ_LEN     = 1_024\n","BATCH_SIZE  = 5\n","MAX_STEPS   = 1\n","TOP_K       = 3\n","V_SELECT    = \"all\"          # \"all\" | int | list[int] (1-based ranks)\n","NUM_TRIALS  = 2\n","TARGET      = \"transformer.h.0.mlp.c_fc.weight\"\n","\n","# ---- logging of token-level examples -----------------------\n","LOG_EXAMPLES               = True\n","EXAMPLES_PER_CLASS         = 1     # print examples for first N finite trials / class\n","EXAMPLE_BATCH_INDEX        = 0     # which saved batch to inspect\n","EXAMPLE_TOKENS_PER_BATCH   = 20    # how many positions to print\n","EXAMPLE_CONTEXT_TOKENS     = 20    # context tail to show before position\n","\n","# ============================================================\n","# 2. Model & tokenizer\n","# ============================================================\n","tok = AutoTokenizer.from_pretrained(MODEL_ID)\n","tok.pad_token = tok.eos_token\n","model = GPT2LMHeadModel.from_pretrained(MODEL_ID).to(DEVICE)\n","model.eval()\n","\n","# ============================================================\n","# 3. Load WikiText-103 locally\n","# ============================================================\n","wiki = load_dataset(\"wikitext\", \"wikitext-103-raw-v1\", split=SPLIT)\n","\n","def chunk_generator():\n","    cache = []\n","    for doc in wiki:\n","        cache.extend(tok(doc[\"text\"]).input_ids)\n","        while len(cache) >= SEQ_LEN + 1:\n","            win, cache = cache[:SEQ_LEN + 1], cache[SEQ_LEN + 1:]\n","            yield win[:-1], win[1:]\n","\n","def get_batch(gen, bs=BATCH_SIZE):\n","    buf = []\n","    for x, _ in gen:\n","        buf.append(x)\n","        if len(buf) == bs:\n","            yield torch.tensor(buf, device=DEVICE)\n","            buf = []\n","\n","# ============================================================\n","# 4. Scan MAX_STEPS → collect per-entry max |grad|\n","# ============================================================\n","W = dict(model.named_parameters())[TARGET]\n","running_max = torch.zeros_like(W)\n","eval_inputs = []\n","\n","for step, inp in enumerate(get_batch(chunk_generator()), 1):\n","    eval_inputs.append(inp)\n","    model.zero_grad(set_to_none=True)\n","    loss = model(inp, labels=inp).loss\n","    loss.backward()\n","    running_max = torch.maximum(running_max, W.grad.abs())\n","    if step >= MAX_STEPS:\n","        break\n","\n","flat_vals, flat_idx = torch.topk(running_max.view(-1), k=TOP_K)\n","coords_list = [torch.unravel_index(i, W.shape) for i in flat_idx]\n","\n","print(f\"\\nTop-{TOP_K} |∂L/∂θ| entries in {TARGET}:\")\n","for rank, (val, c) in enumerate(zip(flat_vals.tolist(), coords_list), 1):\n","    r, q = map(int, c)\n","    print(f\"  #{rank}: {TARGET}({r},{q})  |grad|={val:.3e}\")\n","\n","def normalize_v_select(v_sel, k):\n","    if v_sel == \"all\": return list(range(1, k+1))\n","    if isinstance(v_sel, int): return [v_sel]\n","    if isinstance(v_sel, (list, tuple)): return list(v_sel)\n","    raise ValueError(\"V_SELECT must be 'all', int, or list[int]\")\n","\n","ranks_to_test = normalize_v_select(V_SELECT, TOP_K)\n","print(f\"\\nTesting ranks: {ranks_to_test}\")\n","\n","# ============================================================\n","# 5. Perplexity over all saved inputs (explicit shift)\n","# ============================================================\n","@torch.no_grad()\n","def ppl_over_inputs(batches):\n","    total_nll, total_tok = 0.0, 0\n","    for x in batches:\n","        logits = model(x[:, :-1]).logits\n","        tgt    = x[:, 1:]\n","        nll = F.cross_entropy(\n","            logits.reshape(-1, logits.size(-1)),\n","            tgt.reshape(-1),\n","            reduction=\"sum\"\n","        )\n","        total_nll += nll.item()\n","        total_tok += tgt.numel()\n","    return math.exp(total_nll / total_tok)\n","\n","baseline_ppl = ppl_over_inputs(eval_inputs)\n","print(f\"\\nBaseline PPL over {len(eval_inputs)*BATCH_SIZE} windows: {baseline_ppl:.4f}\")\n","\n","# ============================================================\n","# 6. Helpers: bit flip, decode float, and example printer\n","# ============================================================\n","def flip_bit(val_tensor: torch.Tensor, bit: int):\n","    iv = val_tensor.view(torch.int32)\n","    iv ^= (1 << bit)\n","    return iv.view(torch.float32)\n","\n","def decode_float32(t: torch.Tensor):\n","    iv = int(t.view(torch.int32).item())\n","    s  = (iv >> 31) & 0x1\n","    E  = (iv >> 23) & 0xFF\n","    M  = iv & 0x7FFFFF\n","    return s, E, M, iv\n","\n","@torch.no_grad()\n","def print_token_examples(title: str):\n","    \"\"\"Print a few token-level comparisons for one saved batch.\"\"\"\n","    if not LOG_EXAMPLES: return\n","    if EXAMPLE_BATCH_INDEX >= len(eval_inputs): return\n","    x = eval_inputs[EXAMPLE_BATCH_INDEX]                      # [B,1024]\n","    logits = model(x[:, :-1]).logits                          # [B,1023,V]\n","    tgt    = x[:, 1:]                                         # [B,1023]\n","    pred   = logits.argmax(dim=-1)                            # [B,1023]\n","    b = 0                                                     # first item in batch\n","    T = min(EXAMPLE_TOKENS_PER_BATCH, pred.size(1))\n","    print(f\"\\n--- {title}  (batch={EXAMPLE_BATCH_INDEX}, item=0, {T} positions) ---\")\n","    for t in range(T):\n","        ctx_start = max(0, t+1-EXAMPLE_CONTEXT_TOKENS)\n","        ctx_ids   = x[b, ctx_start:t+1].tolist()\n","        ctx_text  = tok.decode(ctx_ids)\n","        truth_id  = int(tgt[b, t].item())\n","        pred_id   = int(pred[b, t].item())\n","        # probabilities\n","        probs = torch.softmax(logits[b, t], dim=-1)\n","        p_truth = float(probs[truth_id].item())\n","        p_pred  = float(probs[pred_id].item())\n","        truth_tok = tok.convert_ids_to_tokens(truth_id)\n","        pred_tok  = tok.convert_ids_to_tokens(pred_id)\n","        print(f\"[t={t:4d}] ctx: {ctx_text!r}\")\n","        print(f\"        truth: {truth_tok:<12}  pred: {pred_tok:<12}  \"\n","              f\"P(truth)={p_truth:.4f}  P(pred)={p_pred:.4f}\")\n","\n","BIT_CLASSES = {\n","    \"sign\"     : [31],\n","    \"exponent\" : list(range(23, 31)),\n","    \"mantissa\" : list(range(0, 23)),\n","}\n","\n","# ============================================================\n","# 7. Flip bits – skip NaNs, record failures, and print examples\n","# ============================================================\n","overall = {}   # {rank: {cls: {\"mean\":..., \"fails\":...}}}\n","\n","for rank in ranks_to_test:\n","    coord    = coords_list[rank-1]\n","    orig_val = W.data[coord].clone()\n","    s0,E0,M0,iv0 = decode_float32(orig_val)\n","    print(f\"\\nRank #{rank}  coord={TARGET}{tuple(map(int,coord))}  \"\n","          f\"orig={float(orig_val.item()):+.6e}  (s={s0}, E={E0}, M=0x{M0:06x})\")\n","\n","    cls_stats = {}\n","    printed_count = {k: 0 for k in BIT_CLASSES}  # limit example logs\n","\n","    for cls, pool in BIT_CLASSES.items():\n","        deltas, fails = [], 0\n","        for _ in range(NUM_TRIALS):\n","            bit = random.choice(pool)\n","            # corrupt\n","            W.data[coord] = flip_bit(orig_val, bit)\n","            s1,E1,M1,iv1 = decode_float32(W.data[coord])\n","            ppl_c = ppl_over_inputs(eval_inputs)\n","\n","            if math.isfinite(ppl_c):\n","                delta = ppl_c - baseline_ppl\n","                deltas.append(delta)\n","                # print examples once per class (or up to EXAMPLES_PER_CLASS)\n","                if LOG_EXAMPLES and printed_count[cls] < EXAMPLES_PER_CLASS:\n","                    title = (f\"{cls} flip bit {bit}  \"\n","                             f\"val {float(orig_val.item()):+.3e} -> \"\n","                             f\"{float(W.data[coord].item()):+.3e}  \"\n","                             f\"(s:{s0}->{s1}, E:{E0}->{E1}, M:0x{M0:06x}->0x{M1:06x})  \"\n","                             f\"ΔPPL={delta:+.4f}\")\n","                    print_token_examples(title)\n","                    printed_count[cls] += 1\n","            else:\n","                fails += 1\n","\n","            # restore\n","            W.data[coord] = orig_val\n","\n","        mean_delta = (sum(deltas) / len(deltas)) if deltas else float(\"nan\")\n","        cls_stats[cls] = {\"mean\": mean_delta, \"fails\": fails}\n","\n","    overall[rank] = cls_stats\n","\n","# ============================================================\n","# 8. Report\n","# ============================================================\n","print(\"\\n=== Mean Δ‑PPL (finite trials only) ===\")\n","for rank in sorted(overall):\n","    r, c = map(int, coords_list[rank-1])\n","    print(f\"\\nRank #{rank}  ({TARGET}({r},{c}))\")\n","    for cls, st in overall[rank].items():\n","        mean_str = f\"{st['mean']:+.4f}\" if math.isfinite(st['mean']) else \"NaN\"\n","        print(f\"  {cls:9s}: {mean_str:>10}   (skipped {st['fails']}/{NUM_TRIALS})\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"0tekTWdisLVD","executionInfo":{"status":"ok","timestamp":1753465696341,"user_tz":-330,"elapsed":8751,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}},"outputId":"514b01aa-2dca-418d-abac-f2bcbce96b24"},"execution_count":5,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Top-3 |∂L/∂θ| entries in transformer.h.0.mlp.c_fc.weight:\n