"import itertools\n","import matplotlib.pyplot as plt\n","import pandas as pd\n","import math\n","\n","# -------------------------------\n","# Token Setup\n","# -------------------------------\n","TOKENS = [0, 1, \"END\"]\n","END_IDX = 2\n","NUM_TOK = len(TOKENS)\n","MAX_LEN = 4  # Max sequence length excluding END\n","\n","# -------------------------------\n","# Transition Matrices\n","# -------------------------------\n","P = np.array([\n","    [0.4, 0.4, 0.2],  # From 0\n","    [0.3, 0.5, 0.2],  # From 1\n","    [0.4, 0.4, 0.2]   # From END (used as START)\n","])\n","\n","Q = np.array([\n","    [0.5, 0.3, 0.2],  # From 0\n","    [0.2, 0.6, 0.2],  # From 1\n","    [0.4, 0.4, 0.2]   # From END (used as START)\n","])\n","\n","# -------------------------------\n","# Sequence Generation\n","# -------------------------------\n","def generate_sequences():\n","    sequences = []\n","    for length in range(1, MAX_LEN + 1):\n","        for seq in itertools.product([0, 1], repeat=length):\n","            sequences.append(list(seq) + [END_IDX])\n","    return sequences\n","\n","# -------------------------------\n","# Sequence Probability\n","# -------------------------------\n","def sequence_prob(seq, matrix):\n","    prob = 1.0\n","    prev = END_IDX  # Start from END for convenience\n","    for tok in seq:\n","        prob *= matrix[prev][tok]\n","        prev = tok\n","    return prob\n","\n","# -------------------------------\n","# Bernoulli KL Divergence\n","# -------------------------------\n","def bernoulli_kl(p, q):\n","    if p == 0:\n","        return math.log((1 - p) / (1 - q)) if q != 1 else float('inf')\n","    elif p == 1:\n","        return math.log(p / q) if q != 0 else float('inf')\n","    else:\n","        return p * math.log(p / q) + (1 - p) * math.log((1 - p) / (1 - q))\n","\n","# -------------------------------\n","# Main Computation\n","# -------------------------------\n","sequences = generate_sequences()\n","probs_p = [sequence_prob(seq, P) for seq in sequences]\n","probs_q = [sequence_prob(seq, Q) for seq in sequences]\n","kl_bernoulli = [bernoulli_kl(p, q) for p, q in zip(probs_p, probs_q)]\n","\n","# KL Divergence between categorical distributions\n","kl_categorical = sum(p * math.log(p / q) for p, q in zip(probs_p, probs_q))\n","print(f\"Total KL divergence between P(x) and Q(x): {kl_categorical:.6f}\")\n","\n","# Create DataFrame for viewing\n","df = pd.DataFrame({\n","    \"Sequence\": [\"\".join(map(str, seq[:-1])) for seq in sequences],\n","    \"P(x)\": probs_p,\n","    \"Q(x)\": probs_q,\n","    \"Bernoulli_KL(P||Q)\": kl_bernoulli\n","})\n","print(df)\n","\n","# -------------------------------\n","# Plot Bernoulli KL Divergence\n","# -------------------------------\n","plt.figure(figsize=(10, 5))\n","plt.plot(range(len(kl_bernoulli)), kl_bernoulli, marker='o')\n","plt.title(\"Bernoulli KL Divergence per Sequence\")\n","plt.xlabel(\"Sequence Index\")\n","plt.ylabel(\"Bernoulli KL(P||Q)\")\n","plt.grid(True)\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"hiHxUSov0L0f","executionInfo":{"status":"ok","timestamp":1746723502353,"user_tz":-330,"elapsed":401,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}},"outputId":"51906545-ad61-4612-a481-2237a3fc1542"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Total KL divergence between P(x) and Q(x): 0.020457\n","   Sequence     P(x)     Q(x)  Bernoulli_KL(P||Q)\n","0         0  0.08000  0.08000            0.000000\n","1         1  0.08000  0.08000            0.000000\n","2        00  0.03200  0.04000            0.000893\n","3        01  0.03200  0.02400            0.001239\n","4        10  0.02400  0.01600            0.001764\n","5        11  0.04000  0.04800            0.000741\n","6       000  0.01280  0.02000            0.001514\n","7       001  0.01280  0.01200            0.000026\n","8       010  0.00960  0.00480            0.001866\n","9       011  0.01600  0.01440            0.000087\n","10      100  0.00960  0.00800            0.000152\n","11      101  0.00960  0.00480            0.001866\n","12      110  0.01200  0.00960            0.000281\n","13      111  0.02000  0.02880            0.001547\n","14     0000  0.00512  0.01000            0.001465\n","15     0001  0.00512  0.00600            0.000068\n","16     0010  0.00384  0.00240            0.000366\n","17     0011  0.00640  0.00720            0.000047\n","18     0100  0.00384  0.00240            0.000366\n","19     0101  0.00384  0.00144            0.001369\n","20     0110  0.00480  0.00288            0.000534\n","21     0111  0.00800  0.00864            0.000025\n","22     1000  0.00384  0.00400            0.000003\n","23     1001  0.00384  0.00240            0.000366\n","24     1010  0.00288  0.00096            0.001246\n","25     1011  0.00480  0.00288            0.000534\n","26     1100  0.00480  0.00480            0.000000\n","27     1101  0.00480  0.00288            0.000534\n","28     1110  0.00600  0.00576            0.000005\n","29     1111  0.01000  0.01728            0.001837\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x500 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import math, struct, random, psutil, torch, torch.nn.functional as F\n","from transformers import GPT2LMHeadModel, GPT2Tokenizer\n","\n","# ─── CONFIG ──────────────────────────────────────────────────────────────\n","PROMPT          = \"The weather today is \"\n","SEQ_LEN         = 1019\n","SAMPLING_MODE   = \"top-p\"        # \"greedy\" | \"top-k\" | \"top-p\"\n","TOP_K, TOP_P    = 10, 0.9\n","EPS             = 1e-12\n","\n","LAYER_IDX       = 5\n","WEIGHT_NAME     = \"mlp.c_fc.weight\"\n","FLAT_INDEX      = 42\n","BIT_POS         = 5\n","\n","MEM_FRAC        = 0.10\n","MAX_SEQ_LIMIT   = 10\n","PRINT_EVERY     = 100\n","# ─────────────────────────────────────────────────────────────────────────\n","\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","tok    = GPT2Tokenizer.from_pretrained(\"gpt2\")\n","EOS_ID = tok.eos_token_id\n","\n","# two independent copies\n","model_clean   = GPT2LMHeadModel.from_pretrained(\"gpt2\").eval().to(device)\n","model_corrupt = GPT2LMHeadModel.from_pretrained(\"gpt2\").eval().to(device)\n","\n","prompt_ids = tok(PROMPT, return_tensors=\"pt\")[\"input_ids\"].to(device)\n","# CTX_MAX    = model_clean.config.n_positions\n","# SEQ_LEN    = min(SEQ_LEN, CTX_MAX - prompt_ids.size(1))\n","\n","# ─── apply ONE bit-flip to corrupt model only ────────────────────────────\n","def flip_bit(tensor, flat_idx, bit_pos):\n","    with torch.no_grad():\n","        data = tensor.view(-1)\n","        ival = struct.unpack('I', struct.pack('f', data[flat_idx].item()))[0]\n","        data[flat_idx] = struct.unpack(\n","            'f', struct.pack('I', ival ^ (1 << bit_pos))\n","        )[0]\n","\n","flip_bit(eval(f\"model_corrupt.transformer.h[{LAYER_IDX}].{WEIGHT_NAME}\"),\n","         FLAT_INDEX, BIT_POS)\n","\n","# ─── sampling helpers ────────────────────────────────────────────────────\n","def choose(probs, fixed=None):\n","    if fixed is not None:\n","        return fixed\n","    if SAMPLING_MODE == \"greedy\":\n","        return torch.argmax(probs).item()\n","    sorted_p, sorted_i = torch.sort(probs, descending=True)\n","    if SAMPLING_MODE == \"top-k\":\n","        cand_i, cand_p = sorted_i[:TOP_K], sorted_p[:TOP_K]\n","    else:\n","        cs = torch.cumsum(sorted_p, -1)\n","        cut = (cs > TOP_P).nonzero(as_tuple=True)\n","        cut = cut[0][0].item() + 1 if cut[0].numel() else len(sorted_p)\n","        cand_i, cand_p = sorted_i[:cut], sorted_p[:cut]\n","    return cand_i[torch.multinomial(cand_p, 1)].item()\n","\n","def generate(model, fixed_seq=None):\n","    \"\"\"returns (token_list, log2P)\"\"\"\n","    ids, l2p, out = prompt_ids.clone(), 0.0, []\n","    for t in range(SEQ_LEN):\n","        with torch.no_grad():\n","            probs = F.softmax(model(ids).logits[0, -1], dim=-1)\n","        token = choose(probs, fixed_seq[t] if fixed_seq else None)\n","        l2p  += math.log2(probs[token].item() + EPS)\n","        out.append(token)\n","        if token == EOS_ID:\n","            break\n","        ids = torch.cat([ids, torch.tensor([[token]], device=device)], 1)\n","    return out, l2p\n","\n","# ─── numerically-stable log2 KL accumulator ──────────────────────────────\n","class Log2KL:\n","    def __init__(self): self.init=False\n","    def _rescale(self,a):\n","        if not self.init: self.anchor=a; self.sp=self.sq=self.kl=0.0; self.init=True; return\n","        s=2**(self.anchor-a); self.sp*=s; self.sq*=s; self.kl*=s; self.anchor=a\n","    def add(self,l2p,l2q):\n","        if not self.init or l2p>self.anchor: self._rescale(l2p)\n","        p=2**(l2p-self.anchor); q=2**(l2q-self.anchor)\n","        self.sp+=p; self.sq+=q; self.kl+=p*(l2p-l2q)\n","    def results(self):\n","        fac = 2**self.anchor if self.init else 1.0\n","        return self.kl, self.sp*fac, self.sq*fac\n","\n","def host_free_ratio():\n","    vm = psutil.virtual_memory()\n","    return vm.available / vm.total\n","\n","# ─── main loop ───────────────────────────────────────────────────────────\n","kl_acc, n_seq = Log2KL(), 0\n","\n","while n_seq < MAX_SEQ_LIMIT and host_free_ratio() > (1 - MEM_FRAC):\n","    tokens, log2P = generate(model_clean)\n","    _,      log2Q = generate(model_corrupt, fixed_seq=tokens)\n","    kl_acc.add(log2P, log2Q)\n","    n_seq += 1\n","    if n_seq % PRINT_EVERY == 0:\n","        print(f\"{n_seq} seq • free RAM {host_free_ratio():.2f}\")\n","\n","# ─── complementary lump & report ─────────────────────────────────────────\n","KL_bits, P_mass, Q_mass = kl_acc.results()\n","p_star, q_star = max(0.,1-P_mass), max(0.,1-Q_mass)\n","KL_bits += p_star * math.log2((p_star+EPS)/(q_star+EPS))\n","\n","print(\"\\n──────── SUMMARY ────────\")\n","print(f\"Sequences processed  : {n_seq}\")\n","print(f\"ΣP_mass (clean)      : {P_mass:.9e}\")\n","print(f\"ΣQ_mass (corrupt)    : {Q_mass:.9e}\")\n","print(f\"Complement masses P★ : {p_star:.9e}\")\n","print(f\"                  Q★ : {q_star:.9e}\")\n","print(f\"\\nAggregated KL (bits) : {KL_bits:.10f}\")\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ri_dX2zYuHVJ","executionInfo":{"status":"ok","timestamp":1747583012901,"user_tz":-330,"elapsed":137767,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}},"outputId":"89ccfb77-a418-4623-ae88-67efcc912f93"},"execution_count":11,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","──────── SUMMARY ────────\n","Sequences processed  : 10\n","ΣP_mass (clean)      : 2.426416479e-181\n","ΣQ_mass (corrupt)    : 2.426381874e-181\n","Complement masses P★ : 1.000000000e+00\n","                  Q★ : 1.000000000e+00\n","\n","Aggregated KL (bits) : 0.0000205756\n"]}]}]}