ctivation(output.clone())\n","        return output\n","    return hook\n","\n","# Generation function with hook and token table output\n","def generate_with_table(model, input_ids, inject_flip=False):\n","    tables = []\n","    cur_input_ids = input_ids.clone()\n","\n","    for step in range(num_generate):\n","        # Hook setup\n","        hooks = []\n","        for i in range(len(model.transformer.h)):\n","            flip_now = inject_flip and (i == layer_to_flip)\n","            hook = model.transformer.h[i].register_forward_hook(make_hook(flip=flip_now))\n","            hooks.append(hook)\n","\n","        # Forward pass\n","        with torch.no_grad():\n","            outputs = model(cur_input_ids)\n","            logits = outputs.logits\n","            probs = F.softmax(logits[0, -1, :], dim=-1)\n","\n","        for h in hooks:\n","            h.remove()\n","\n","        # Sampling\n","        if decoding_strategy == \"top-k\":\n","            sampled_probs, sampled_indices = torch.topk(probs, top_k)\n","            sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","            sampled_token = sampled_indices[sampled_pos]\n","        elif decoding_strategy == \"top-p\":\n","            sorted_probs, sorted_indices = torch.sort(probs, descending=True)\n","            cumulative_probs = torch.cumsum(sorted_probs, dim=-1)\n","            cutoff_idx = (cumulative_probs > top_p).nonzero(as_tuple=True)[0][0].item() + 1\n","            sampled_probs = sorted_probs[:cutoff_idx]\n","            sampled_indices = sorted_indices[:cutoff_idx]\n","            sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","            sampled_token = sampled_indices[sampled_pos]\n","        else:\n","            sampled_probs, sampled_indices = torch.topk(probs, top_k)\n","            sampled_token = sampled_indices[0]\n","            sampled_pos = 0\n","\n","        # Table construction\n","        token_ids = sampled_indices.tolist()\n","        token_texts = [tokenizer.decode([idx]) for idx in token_ids]\n","        token_probs = sampled_probs.tolist()\n","        chosen_token_id = sampled_token.item()\n","        chosen_token_str = tokenizer.decode([chosen_token_id])\n","        chosen_token_prob = sampled_probs[sampled_pos].item()\n","        highlight_column = ['← chosen' if idx == chosen_token_id else '' for idx in token_ids]\n","\n","        table = pd.DataFrame({\n","            'Token': token_texts,\n","            'Probability': token_probs,\n","            'Generated?': highlight_column\n","        }, index=token_ids)\n","        table.index.name = \"Token ID\"\n","\n","        print(f\"\\n📍 Step {step + 1} | Prompt so far: {tokenizer.decode(cur_input_ids[0])}\")\n","        print(f\"🧠 Generated Token: '{chosen_token_str}' | Token ID: {chosen_token_id} | Probability: {chosen_token_prob:.5f}\")\n","        print(table)\n","\n","        tables.append(table)\n","        cur_input_ids = torch.cat([cur_input_ids, sampled_token.unsqueeze(0).unsqueeze(0)], dim=1)\n","\n","    return tables\n","\n","# Run clean and corrupted generations\n","print(\"\\n🟢 CLEAN RUN\")\n","clean_tables = generate_with_table(model_clean, input_ids, inject_flip=False)\n","\n","print(\"\\n🔴 CORRUPTED RUN (bit flip in Layer {})\".format(layer_to_flip))\n","corrupt_tables = generate_with_table(model_corrupt, input_ids, inject_flip=True)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"3Uj-ExGf9ZbM","executionInfo":{"status":"ok","timestamp":1745330354562,"user_tz":-330,"elapsed":1670,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}},"outputId":"f2a4a76d-0018-4906-ec6e-27a02e22419d"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","🟢 CLEAN RUN\n","\n","📍 Step 1 | Prompt so far: The weather today is\n","🧠 Generated Token: ' pretty' | Token ID: 2495 | Probability: 0.03057\n","                 Token  Probability Generated?\n","Token ID                              