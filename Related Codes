suring Severity Score**"],"metadata":{"id":"Z6sxjMWLherU"}},{"cell_type":"code","source":["import torch\n","import torch.nn.functional as F\n","import numpy as np\n","from transformers import GPT2Tokenizer, GPT2LMHeadModel\n","import struct\n","import random\n","import math\n","\n","# === Configuration ===\n","NUM_TOKENS = 15\n","NUM_RUNS = 1\n","BITS_FLIPPED = 1\n","LAYER_TO_FLIP = 5\n","SAMPLING_MODE = \"top-p\"  # choose from \"top-k\", \"top-p\", \"greedy\"\n","TOP_K = 10\n","TOP_P = 0.9\n","\n","tokenizer = GPT2Tokenizer.from_pretrained(\"gpt2\")\n","model = GPT2LMHeadModel.from_pretrained(\"gpt2\").eval().cuda()\n","input_ids = tokenizer(\"The weather today is\", return_tensors=\"pt\")[\"input_ids\"].to(\"cuda\")\n","\n","# === Bit Flip Function ===\n","def flip_activation(tensor, num_bits=1):\n","    flat = tensor.view(-1)\n","    for _ in range(num_bits):\n","        idx = torch.randint(0, flat.numel(), (1,)).item()\n","        val = flat[idx].item()\n","        int_val = struct.unpack('I', struct.pack('f', val))[0]\n","        bit_pos = torch.randint(0, 32, (1,)).item()\n","        flipped = int_val ^ (1 << bit_pos)\n","        new_val = struct.unpack('f', struct.pack('I', flipped))[0]\n","        flat[idx] = new_val\n","    return tensor\n","\n","# === Hook Registration ===\n","def hook_fn(module, input, output):\n","    if isinstance(output, tuple):\n","        return (flip_activation(output[0].clone()),) + output[1:]\n","    else:\n","        return flip_activation(output.clone())\n","\n","# === KL Divergence Function (with error tracking) ===\n","def compare_distributions_kl_tvd(logits_clean, logits_corrupt, epsilon=1e-10):\n","    if torch.isnan(logits_corrupt).any() or torch.isinf(logits_corrupt).any():\n","        print(\"[ERROR] Corrupted logits contain NaN or Inf.\")\n","        return float('nan'), float('nan')\n","\n","    p_clean = F.softmax(logits_clean, dim=-1)\n","    p_corrupt = F.softmax(logits_corrupt, dim=-1)\n","\n","    if torch.isnan(p_corrupt).any() or torch.isinf(p_corrupt).any():\n","        print(\"[ERROR] Softmax over corrupted logits produced NaNs or Infs.\")\n","        return float('nan'), float('nan')\n","\n","    p_clean = torch.clamp(p_clean, min=epsilon)\n","    p_corrupt = torch.clamp(p_corrupt, min=epsilon)\n","    kl_div = torch.sum(p_clean * torch.log(p_clean / p_corrupt)).item()\n","    tvd = 0.5 * torch.sum(torch.abs(p_clean - p_corrupt)).item()\n","    return kl_div, tvd\n","\n","# === Generation ===\n","def generate_logits(model, input_ids, flip=False):\n","    logits_list = []\n","    cur_input_ids = input_ids.clone()\n","    for step in range(NUM_TOKENS):\n","        handle = None\n","        if flip and step == 0:\n","            handle = model.transformer.h[LAYER_TO_FLIP].register_forward_hook(hook_fn)\n","\n","        with torch.no_grad():\n","            outputs = model(cur_input_ids)\n","            logits = outputs.logits\n","            last_logits = logits[0, -1, :]\n","            probs = F.softmax(last_logits, dim=-1)\n","\n","        if handle:\n","            handle.remove()\n","\n","        sorted_probs, sorted_indices = torch.sort(probs, descending=True)\n","        cumulative_probs = torch.cumsum(sorted_probs, dim=-1)\n","        cutoff_idx = (cumulative_probs > TOP_P).nonzero(as_tuple=True)\n","\n","        if SAMPLING_MODE == \"top-p\":\n","            if cutoff_idx[0].numel() == 0:\n","                print(f\"[WARNING] Fallback to greedy at step {step}.\")\n","                next_token = sorted_indices[0].unsqueeze(0).unsqueeze(0)\n","            else:\n","                cutoff = cutoff_idx[0][0].item() + 1\n","                sampled_probs = sorted_probs[:cutoff]\n","                sampled_indices = sorted_indices[:cutoff]\n","                sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","                next_token = sampled_indices[sampled_pos].unsqueeze(0).unsqueeze(0)\n","\n","        elif SAMPLING_MODE == \"top-k\":\n","            sampled_probs, sampled_indices = torch.topk(probs, TOP_K)\n","            sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","            next_token = sampled_indices[sampled_pos].unsqueeze(0).unsqueeze(0)\n","\n","        else:\n","            next_token = sorted_indices[0].unsqueeze(0).unsqueeze(0)\n","\n","        logits_list.append(last_logits)\n","        cur_input_ids = torch.cat([cur_input_ids, next_token], dim=1)\n","    return logits_list\n","\n","# === Run Experiment ===\n","def run_experiment():\n","    clean_kls = []\n","    corrupt_kls = []\n","\n","    for _ in range(NUM_RUNS):\n","        logits_clean_1 = generate_logits(model, input_ids, flip=False)\n","        logits_clean_2 = generate_logits(model, input_ids, flip=False)\n","        logits_corrupt = generate_logits(model, input_ids, flip=True)\n","\n","        run_kls_clean_vs_clean = []\n","        run_kls_clean_vs_corrupt = []\n","\n","        for l1, l2, lc in zip(logits_clean_1, logits_clean_2, logits_corrupt):\n","            kl_clean = compare_distributions_kl_tvd(l1, l2)[0]\n","            kl_corrupt = compare_distributions_kl_tvd(l1, lc)[0]\n","\n","            if not math.isnan(kl_clean):\n","                run_kls_clean_vs_clean.append(kl_clean)\n","            if not math.isnan(kl_corrupt):\n","                run_kls_clean_vs_corrupt.append(kl_corrupt)\n","\n","        if run_kls_clean_vs_clean:\n","            clean_kls.append(sum(run_kls_clean_vs_clean) / len(run_kls_clean_vs_clean))\n","        if run_kls_clean_vs_corrupt:\n","            corrupt_kls.append(sum(run_kls_clean_vs_corrupt) / len(run_kls_clean_vs_corrupt))\n","\n","    return clean_kls, corrupt_kls\n","\n","# === Run and print ===\n","clean_kls, corrupt_kls = run_experiment()\n","\n","print(\"\\nMean Natural KL:\", np.mean(clean_kls))\n","print(\"Mean Bit-Flip KL:\", np.mean(corrupt_kls))\n","\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"X02b7gsdXcde","executionInfo":{"status":"ok","timestamp":1745167934310,"user_tz":-330,"elapsed":1943,"user":{"displayName":"Aryash Raghavendra","userId":"07735685809269068536"}},"outputId":"8745a95f-d0d8-4f21-f684-8c26839d23f7"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Mean Natural KL: 5.459334945678711\n","Mean Bit-Flip KL: 5.67960833763985\n"]}]},{"cell_type":"markdown","source":["# **Exploring effects of targeted bit flips**"],"metadata":{"id":"ZQ43F1Eh316X"}},{"cell_type":"code","source":["import torch\n","import torch.nn.functional as F\n","import struct\n","import random\n","import pandas as pd\n","from transformers import GPT2LMHeadModel, GPT2Tokenizer\n","\n","# === CONFIGURATION ===\n","NUM_TOKENS = 25\n","BITS_FLIPPED = 1\n","TARGET_LAYER = 5\n","TARGET_WEIGHT_NAME = \"mlp.c_fc.weight\"\n","SAMPLING_MODE = \"top-p\"  # \"top-k\", \"top-p\", or \"greedy\"\n","TOP_K = 10\n","TOP_P = 0.9\n","EPSILON = 1e-10\n","NUM_RUNS = 5\n","\n","# === SETUP ===\n","tokenizer = GPT2Tokenizer.from_pretrained(\"gpt2\")\n","model = GPT2LMHeadModel.from_pretrained(\"gpt2\").eval().cuda()\n","input_ids = tokenizer(\"The weather today is\", return_tensors=\"pt\")[\"input_ids\"].to(\"cuda\")\n","\n","# === FLOAT UTILITIES ===\n","def float_to_bits(f):\n","    return struct.unpack('I', struct.pack('f', f))[0]\n","\n","def bits_to_float(b):\n","    return struct.unpack('f', struct.pack('I', b))[0]\n","\n","# === BIT FLIP FUNCTION ===\n","def flip_tensor_bit(tensor, flip_type):\n","    with torch.no_grad():\n","        flat = tensor.view(-1)\n","        idx = random.randint(0, flat.numel() - 1)\n","        val = flat[idx].item()\n","        bits = float_to_bits(val)\n","        if flip_type == 'sign':\n","            bit = 31\n","        elif flip_type == 'exp':\n","            bit = random.randint(23, 30)\n","        elif flip_type == 'mant':\n","            bit = random.randint(0, 22)\n","        else:\n","            raise ValueError(\"Unknown flip_type\")\n","        flipped = bits ^ (1 << bit)\n","        new_val = bits_to_float(flipped)\n","        flat[idx] = new_val\n","        return idx, val, new_val, bit\n","\n","# === SAMPLING FUNCTION (fixed) ===\n","def sample_next_token(probs):\n","    sorted_probs, sorted_indices = torch.sort(probs, descending=True)\n","    if SAMPLING_MODE == \"top-k\":\n","        sampled_probs, sampled_indices = torch.topk(probs, TOP_K)\n","        sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","        return sampled_indices[sampled_pos].view(1, 1)\n","\n","    elif SAMPLING_MODE == \"top-p\":\n","        cumulative_probs = torch.cumsum(sorted_probs, dim=-1)\n","        cutoff = (cumulative_probs > TOP_P).nonzero(as_tuple=True)\n","        if cutoff[0].numel() == 0:\n","            print(\"[WARNING] Fallback to greedy\")\n","            return sorted_indices[0].view(1, 1)\n","        cutoff = cutoff[0][0].item() + 1\n","        sampled_probs = sorted_probs[:cutoff]\n","        sampled_indices = sorted_indices[:cutoff]\n","        sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","        return sampled_indices[sampled_pos].view(1, 1)\n","\n","    else:  # greedy\n","        return sorted_indices[0].view(1, 1)\n","\n","# === GENERATE LOGITS + TOKENS ===\n","def generate_logits_sequence(model, input_ids, num_tokens):\n","    logits_list = []\n","    tokens = []\n","    cur_input_ids = input_ids.clone()\n","    for _ in range(num_tokens):\n","        with torch.no_grad():\n","            outputs = model(cur_input_ids)\n","            logits = outputs.logits\n","            last_logits = logits[0, -1, :]\n","        logits_list.append(last_logits)\n","        next_token = sample_next_token(F.softmax(last_logits, dim=-1))\n","        tokens.append(next_token.item())\n","        cur_input_ids = torch.cat([cur_input_ids, next_token], dim=1)\n","    return logits_list, tokens\n","\n","# === KL Divergence ===\n","def kl_divergence(p_logits, q_logits):\n","    p = F.softmax(p_logits, dim=-1)\n","    q = F.softmax(q_logits, dim=-1)\n","    p = torch.clamp(p, min=EPSILON)\n","    q = torch.clamp(q, min=EPSILON)\n","    return torch.sum(p * torch.log(p / q)).item()\n","\n","# === FIXED CLEAN REFERENCE ===\n","fixed_clean_logits, fixed_clean_tokens = generate_logits_sequence(model, input_ids, NUM_TOKENS)\n","fixed_clean_text = tokenizer.decode(fixed_clean_tokens)\n","\n","# === Target weight ===\n","weight_tensor = eval(f\"model.transformer.h[{TARGET_LAYER}].{TARGET_WEIGHT_NAME}\")\n","original_data = weight_tensor.data.clone()\n","\n","# === Run Experiments ===\n","results = []\n","for run in range(NUM_RUNS):\n","    for flip_type in ['sign', 'exp', 'mant']:\n","        idx, old_val, new_val, bit = flip_tensor_bit(weight_tensor, flip_type)\n","        corrupt_logits, corrupt_tokens = generate_logits_sequence(model, input_ids, NUM_TOKENS)\n","        corrupt_text = tokenizer.decode(corrupt_tokens)\n","        weight_tensor.data.copy_(original_data)\n","\n","        kls = []\n","        for i, (clean_logit, corrupt_logit) in enumerate(zip(fixed_clean_logits, corrupt_logits)):\n","            kl = kl_divergence(clean_logit, corrupt_logit)\n","            kls.append(kl)\n","\n","        results.append({\n","            \"Run\": run,\n","            \"Flip Type\": flip_type,\n","            \"Bit Pos\": bit,\n","            \"Weight Index\": idx,\n","            \"Old Val\": old_val,\n","            \"New Val\": new_val,\n","            \"Severity Score (Avg KL)\": sum(kls)/len(kls),\n","            \"Clean Output\": fixed_clean_text,\n","            \"Corrupt Output\": corrupt_text\n","        })\n","\n","# === Display Final Table ===\n","df = pd.DataFrame(results)\n","pd.set_option(\"display.max_colwidth\", None)\n","print(df.to_string(index=False))\n","\n","# === Print Mean KL by Flip Type ===\n","print(\"\\n=== Mean KL Divergence by Flip Type ===\")\n","mean_by_type = df.groupby(\"Flip Type\")[\"Severity Score (Avg KL)\"].mean()\n","for flip_type, mean_kl in mean_by_type.items():\n","    print(f\"{flip_type:<6} → {mean_kl:.10f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":764,"referenced_widgets":["736c3533232e4ad3bae241aad0bc27e2","d7762a85a96542d38eb48b7d6605e6a5","2888ce24ab9e4ff49c0ace6ca10a3823","e71cc434974e4c49baf07f83a8f73f40","22279f330d2a435eb9ddbf0e6731584d","103281053eb640beb8e97d40b2bde023","c2acea4640664678ad4c280f51715b2b","5cc57b4978814785b5a748ca7df76fd9","c782f14f391244559493f0fb0164b34f","3a4b58da1800499aacd5ed6b54648475","320d943e309a443e87ae96a4f20f7ab6","f84e1e3098c7429da4abc87bc3c00326","017d1bdcfa624e42a4f78986f128dbb3","143c02c0d9134697881e7c0b8e65b291","0337b8eb28a84b9b8db19ce9046b9744","db3e2b6d5d7e43128a1b7fea1c9322f2","d366ad61925f4c1e9f5fb1395a4dc3c2","c7f416ba26464de2a756e648faa1cb7c","308a5a2dbebb4b10937f6758cf7670c9","441b25485c8942ee9156960503af79e3","fd984f46ebc340ee99634e24f198f9cb","1cf680fc91c244e3a1a50e69580ae192","180b731dd0a34594a0f83e3fcda507d5","0bdef2220dc64f20948103cecc669445","501bba3980db41129221288d653247aa","01b4e246862f4b13bc4ab81351558822","2508b74545ec4fb2b50c9f1a8170f3fb","21725b92fc244066813b14b5518602a4","bec03c370c7c4d6dbc0dc2c357f57690","4afbfe8eaac64399990167fbc122ddae","84f9b76aed024653b70211a75654e1c6","7b9448c4c0b84afd85f29c25d9d99ef8","0659a75cf3394d50abc6bad894a3723e","aff4c3a86e084033a35e086fb864f896","6c8f6ba3b05a4ee8a0c18fa2b6def880","618efe508b1744de9d2fe92ac6da16ad","fec22c2375dd4641a899a3da9d7e57ca","7c0b16059024423b9a710a8a16437933","3472fce339804cc7a57d66680c93161f","f44d8354cd424a8ba3f0b70b80681158","4b0437679cbd4424b13c1cfbe3ff0a8c","563a9b51638a44459ae03f17754757e4","c2bf71cc814d4193984c0293127fe846","966e1461b81f4d019fb90ddbea740f63","c35d9bed90f34591bfe9695ee862916c","4c2e8f1ce15b422c9138a1f75325aaaf","ace308499e534b8db9ec7c49b33a32e6","b12e9a0106bf420d992d2385145085ba","809f02a7599245c0ad7b52db9ec375ef","58c0383c9ee44895a15102a859f8bdaf","ed20a209ed254f7086676b3183524ad2","21a2599fec02438692e8c5e637f1df3b","402d35e7e2