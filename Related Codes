_tz":-330,"elapsed":25394,"user":{"displayName":"Suyash Raghavendra","userId":"02707708531816278458"}},"outputId":"71928120-fb52-4b5f-ecde-4f7092eeaa72","colab":{"base_uri":"https://localhost:8080/"}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Mounted at /content/drive\n"]}]},{"cell_type":"code","source":["import torch\n","from transformers import GPT2Tokenizer, GPT2LMHeadModel\n","import torch.nn.functional as F\n","import pandas as pd\n","\n","# Load model and tokenizer\n","tokenizer = GPT2Tokenizer.from_pretrained(\"gpt2\")\n","model = GPT2LMHeadModel.from_pretrained(\"gpt2\", output_attentions=True).eval().cuda()\n","\n","# Prompt input\n","prompt = \"The weather today is\"\n","input_ids = tokenizer.encode(prompt, return_tensors=\"pt\").to(\"cuda\")\n","\n","# Config\n","num_generate = 20\n","top_k = 10\n","top_p = 0.9\n","decoding_strategy = \"top-p\"  # \"greedy\", \"top-k\", or \"top-p\"\n","\n","# Store token tables\n","token_tables = []\n","\n","# Token-by-token generation\n","for step in range(num_generate):\n","    with torch.no_grad():\n","        outputs = model(input_ids, output_attentions=True)\n","        logits = outputs.logits\n","\n","    next_token_logits = logits[0, -1, :]\n","    probs = F.softmax(next_token_logits, dim=-1)\n","\n","    # Sampling strategies\n","    if decoding_strategy == \"top-k\":\n","        sampled_probs, sampled_indices = torch.topk(probs, top_k)\n","        sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","        sampled_token = sampled_indices[sampled_pos]\n","\n","    elif decoding_strategy == \"top-p\":\n","        sorted_probs, sorted_indices = torch.sort(probs, descending=True)\n","        cumulative_probs = torch.cumsum(sorted_probs, dim=-1)\n","        cutoff_idx = (cumulative_probs > top_p).nonzero(as_tuple=True)[0][0].item() + 1\n","        sampled_probs = sorted_probs[:cutoff_idx]\n","        sampled_indices = sorted_indices[:cutoff_idx]\n","        sampled_pos = torch.multinomial(sampled_probs, 1).item()\n","        sampled_token = sampled_indices[sampled_pos]\n","\n","    else:  # greedy\n","        sampled_probs, sampled_indices = torch.topk(probs, top_k)\n","        sampled_token = sampled_indices[0]\n","        sampled_pos = 0\n","\n","    # Decode token\n","    chosen_token_str = tokenizer.decode([sampled_token.item()])\n","    chosen_token_id = sampled_token.item()\n","    chosen_token_prob = sampled_probs[sampled_pos].item()\n","\n","    # Build table\n","    token_ids = sampled_indices.tolist()\n","    token_texts = [tokenizer.decode([idx]) for idx in token_ids]\n","    token_probs = sampled_probs.tolist()\n","    highlight_column = ['← chosen' if idx == chosen_token_id else '' for idx in token_ids]\n","\n","    display_table = pd.DataFrame({\n","        'Token': token_texts,\n","        'Probability': token_probs,\n","        'Generated': highlight_column\n","    }, index=token_ids)\n","\n","    display_table.index.name = \"Token ID\"\n","\n","    # Print step info\n","    print(f\"\\n📍 Step {step + 1} | Prompt so far: {tokenizer.decode(input_ids[0])}\")\n","    print(f\"🧠 Generated Token: '{chosen_token_str}' | Token ID: {chosen_token_id} | Probability: {chosen_token_prob:.5f}\")\n","    print(display_table)\n","\n","    # Save table and update prompt\n","    token_tables.append(display_table)\n","    input_ids = torch.cat([input_ids, sampled_token.unsqueeze(0).unsqueeze(0)], dim=1)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000,"referenced_widgets":["060dbeda66ed49dd86c5f32c0ad1564b","e7bef3c51fd0401a98abc5f89ecef3fd","59ef35822d0d4c5892b852d603d3956e","7c98a6283f964e2e801ae377ed4dd5c6","2ef2456f92144f18a1a45cfc6836dcbe","a36585ef53744d9c81adbe1de1a6275b","8e7457cb1f1e42bfb02f1d3e96c350b3","1c1b7f60408d4302bb957dbb62510c5c","fcb297fc6d3d4cdfa9836e9a5636c77d","81f2927909d54cfc906dafdc3d430211","9005ab2b6208448798ccb7c48eca3876","3c5d64d71b124fe19f42d4a373585066","974c16049f10456698